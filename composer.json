{
    "name": "innmind/file-watch",
    "type": "library",
    "description": "Library to execute code when a file changes",
    "keywords": ["file", "watch"],
    "homepage": "http://github.com/Innmind/FileWatch",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "baptiste.langlade@hey.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/FileWatch/issues"
    },
    "require": {
        "php": "~8.4",
        "innmind/url": "dev-next",
        "innmind/server-control": "dev-next",
        "innmind/time-warp": "dev-next",
        "innmind/time-continuum": "dev-next",
        "innmind/filesystem": "dev-next",
        "innmind/io": "dev-next",
        "innmind/media-type": "dev-next",
        "innmind/validation": "dev-next",
        "innmind/ip": "dev-next",
        "innmind/immutable": "dev-next",
        "psr/log": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\FileWatch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\FileWatch\\": "tests/"
        }
    },
    "require-dev": {
        "innmind/static-analysis": "^1.2.1",
        "innmind/coding-standard": "~2.0",
        "innmind/black-box": "~6.1"
    }
}
